{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
		"name": "Minshell Launch",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceRoot}/minishell",
		"args": [],
		"stopAtEntry": true,
		"cwd": "${workspaceRoot}",
		"environment": [
		],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			},
			{
				"description": "Set Disassembly Flavor to Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			},
			{
				"description": "Use ld preload for fault injection",
				"text": "-gdb-set environment LD_PRELOAD=./lib/fault_injection/faultinject.so",
				"ignoreFailures": true
			},
			{
				"description": "Set MALLOC_FAIL_RATE",
				"text": "-gdb-set environment MALLOC_FAIL_RATE=0",
				"ignoreFailures": true
			},
			{
				"description": "Set MALLOC_FAIL_VERBOSE",
				"text": "-gdb-set environment MALLOC_FAIL_VERBOSE=1",
				"ignoreFailures": true
			}
		],
		"preLaunchTask": "Default make"
	},
{
	"name": "(don't use)testing Launch",
	"type": "cppdbg",
	"request": "launch",
	"program": "${fileDirname}/a.out",
	"args": [],
	"stopAtEntry": false,
	"cwd": "${workspaceRoot}",
	"environment": [],
	"externalConsole": false,
	"MIMode": "gdb",
	"setupCommands": [
		{
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		},
		{
			"description": "Set Disassembly Flavor to Intel",
			"text": "-gdb-set disassembly-flavor intel",
			"ignoreFailures": true
		}
	],
	"preLaunchTask": "testing make"
}]
}